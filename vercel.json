{
    "rewrites": [
        {
            "source": "/",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "functions": {
        "api/index.js": {
            "includeFiles": "api/*.html"
        }
    }
}